<p id="notice"><%= notice %></p>

<h1>Presences</h1>

<table>
  <thead>
    <tr>
      <th>Asistio</th>
      <th>Fecha</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @presences.each do |presence| %>
      <tr>
        <td><%= presence.asistio %></td>
        <td><%= presence.fecha %></td>
        <td><%= presence.user.name+' '+ presence.user.last_name%></td>
        <td><%= link_to 'Show', profesor_presence_path(@presences) %></td>
        <td><%= link_to 'Destroy', profesor_presence_path(@presences), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Presence', new_profesor_presence_path %>
