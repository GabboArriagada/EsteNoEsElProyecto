<div class="page-header clearfix">
  <h3><%= fa_icon 'leanpub' %> <%= 'Notas' %>
  </h3>
</div>

<div class="panel panel-default" id="table_clientes">
  <div class="panel-heading clearfix">


  </div>

  <div class="panel-body">

    <div class="table-responsive">
      <table class="table table-striped table-hover">
        <thead>
        <tr class="pretty">

      <th>Nombre Asignatura</th>
      <th>Notas Parciales</th>
    </tr>
  </thead>

  <tbody>
    <% @notas.each do |nota| %>
      <tr>
        <td><%= nota.nombre.titleize %></td>
        <td>
          <%= link_to alumno_notas_show_nota_path(asignatura: nota), class: 'btn btn-success btn-xs' do %>
            <%= fa_icon 'search', text: 'Ver' %>
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>

</div>
<div class="panel-footer">

</div>
</div>
